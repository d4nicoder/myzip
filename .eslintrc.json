{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "javascript"
  ],
  "rules": {
      "object-shorthand": "off",
      "semi": "off",
      "space-before-function-paren": "off",
      "no-underscore-dangle": "off",
      "no-param-reassign": "off",
      "no-plusplus": "off",
      "no-console": "off"
  }
}
